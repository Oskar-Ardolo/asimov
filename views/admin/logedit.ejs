<% include ../include/header.ejs %>

<div id="content-wrapper">

<div class="container-fluid">
    <!-- CODE HERE -->
     <ol class="breadcrumb">
      <li class="breadcrumb-item">
        <a href="/">Accueil</a>
      </li>
      <li class="breadcrumb-item">
        <a href="/admin">Admin</a>
      </li>
      <li class="breadcrumb-item">
        <a href="/admin/log">Gestion des logs</a>
      </li>
      <li class="breadcrumb-item active">
        [<%= pseudo %>]-[<%= data.date %>]-[<%= data.id %>]
      </li>
      <li class="breadcrumb-item active"></li>
    </ol>

    <div class="card mb-3">
      <div class="card-header">
        <i class="fas fa-users"></i>
        Gestion des Logs : <i>Logs by Id</i></div>
      <div class="card-body">
        <div class="row">
          <h1 style="float:left;"><%= pseudo %></h1>
        </div>
        <hr>
        <div class="row">
          <table class="table table-bordered">
            <% for (let items in data) {%>
              <% if (items == "description") {%>
                <tr>
                    <td><%= items %></td>
                    <td>
                        <% for (let cases in data.description) { %>
                          <% let fromto = ["From", "To"]%>
                          <% if (data.description.length > 1) {%>
                            <h4><%= fromto[cases] %></h4>
                          <% } %>
                          <table class="table table-bordered">
                            <% for (let detail in data.description[cases]) {%>
                              <tr>
                                <td><%= detail %></td>
                                <td><%= data.description[cases][detail]  %></td>
                              </tr>
                            <% } %>
                          </table>
                        <% } %>
                    </td>
                </tr>
              <% } else { %>
                <tr>
                    <td><%= items %></td>
                    <td><%= data[items] %></td>
                </tr>
              <% } %>
            <% } %>
          </table>
        </div>
      </div>
      <div class="card-footer small text-muted">Mis Ã  jour : il y a quelques secondes...</div>

      </div>
    </div>
    <hr>


</div>


<!-- /.container-fluid -->
<% include ../include/footer.ejs %>
